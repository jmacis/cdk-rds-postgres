{
  "name": "cdk-rds-postgres",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "bin": {
    "cdk-rds-postgres": "bin/cdk-rds-postgres.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "ppredeploy": "npm run build",
    "deploy": "cdk deploy -c env=$NODE_ENV -c vpcid=$npm_config_vpcid --profile $npm_config_profile",
    "deploy:development": "NODE_ENV=development dotenv -e .env.development yarn run deploy",
    "deploy:staging": "NODE_ENV=staging dotenv -e .env.staging yarn run deploy",
    "deploy:production": "NODE_ENV=production dotenv -e .env.production yarn run deploy",
    "synth": "cdk synth -c env=$NODE_ENV -c vpcid=$npm_config_vpcid --profile $npm_config_profile",
    "synth:development": "NODE_ENV=development dotenv -e .env.development yarn run synth",
    "synth:staging": "NODE_ENV=staging dotenv -e .env.staging yarn run synth",
    "synth:production": "NODE_ENV=production dotenv -e .env.production yarn run synth",
    "destroy": "cdk destroy -c env=$NODE_ENV -c vpcid=$npm_config_vpcid --profile $npm_config_profile",
    "destroy:development": "NODE_ENV=development dotenv -e .env.development npm run destroy",
    "destroy:staging": "NODE_ENV=staging dotenv -e .env.staging yarn run destroy",
    "destroy:production": "NODE_ENV=production dotenv -e .env.production yarn run destroy",
    "diff": "cdk diff -c env=$NODE_ENV -c vpcid=$npm_config_vpcid --profile $npm_config_profile",
    "diff:development": "NODE_ENV=development dotenv -e .env.development yarn run diff",
    "diff:staging": "NODE_ENV=staging dotenv -e .env.staging yarn run diff",
    "diff:production": "NODE_ENV=production dotenv -e .env.production yarn run diff",
    "list": "cdk list -c env=$NODE_ENV --profile $npm_config_profile",
    "list:development": "NODE_ENV=development dotenv -e .env.development yarn run list"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.54.0",
    "@types/jest": "^25.2.1",
    "@types/node": "10.17.5",
    "jest": "^25.5.0",
    "ts-jest": "^25.3.1",
    "aws-cdk": "1.54.0",
    "ts-node": "^8.1.0",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "@aws-cdk/aws-autoscaling": "^1.54.0",
    "@aws-cdk/aws-cloudwatch": "^1.54.0",
    "@aws-cdk/aws-cloudwatch-actions": "^1.54.0",
    "@aws-cdk/aws-ec2": "^1.54.0",
    "@aws-cdk/aws-elasticloadbalancingv2": "^1.54.0",
    "@aws-cdk/aws-kms": "^1.54.0",
    "@aws-cdk/aws-lambda": "^1.54.0",
    "@aws-cdk/aws-logs": "^1.54.0",
    "@aws-cdk/aws-rds": "^1.54.0",
    "@aws-cdk/aws-secretsmanager": "^1.54.0",
    "@aws-cdk/aws-sns": "^1.54.0",
    "@aws-cdk/aws-sns-subscriptions": "^1.54.0",
    "@aws-cdk/core": "1.54.0",
    "dotenv-cli": "^3.2.0",
    "source-map-support": "^0.5.16"
  }
}