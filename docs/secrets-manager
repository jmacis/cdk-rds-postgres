aws \
    secretsmanager get-secret-value \
    --secret-id <secret-id> \
    --profile <profile> | \
    jq --raw-output .SecretString | \
    jq -r .\"<key>\"

summon \
    --provider summon-aws-secrets \
    --yaml 'SECRET_KEY: !var <secret-id>' printenv \
    | grep -E '^SECRET_KEY=' \
    | cut -f2 -d= \
    | jq --raw-output . \
    | jq -r .\"<key>\"